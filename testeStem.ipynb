    "import numpy as np\n",
    "\n",
    "%matplotlib inline\n",
    "n = np.linspace(0, 3, 30)\n",
    "x1 = np.sin(10 * np.pi * n) + np.sin(106 * np.pi * n)\n",
    "\n",
    "m, s, b = plt.stem(n, x1, '-.')\n",
    "plt.setp(b, color='r', linewidth=2)\n",
